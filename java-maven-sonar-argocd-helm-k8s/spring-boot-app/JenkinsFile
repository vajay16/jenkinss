pipeline {
  agent any 
  stages {
    stage('Checkout') {
      steps {
        sh 'echo passed'
        checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[credentialsId: 'github', url: 'https://github.com/vajay16/jenkinss']])
      }
    }
   stage('build') {
      steps { 
        sh 'mvn clean install'
      }
     }
   }
}
